@namespace("com.github.devraghav.data_model.domain.project")
protocol Project {
	import idl "../user/User.avdl";
	import idl "Version.avdl";
	record Project {
		string id;
		string name;
		string description;
		boolean enabled;
		string status;
		com.github.devraghav.data_model.domain.user.User author;
		long createdAt;
		union {null,array<Version>} versions;
		union {null,map<string>} tags;
	}
}